{
  "private": false,
  "name": "movijsx",
  "version": "1.1.18",
  "description": "Babel plugin for movi JSX",
  "author": "Ekrem Ershahin <ekremersahin@outlook.com>",
  "homepage": "https://movijs.com/",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.cjs",
      "require": "./dist/index.umd.js"
