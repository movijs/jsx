    "html-tags": "^3.2.0",
    "svg-tags": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@types/jest": "^26.0.24",
    "@types/svg-tags": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
